{
  "name": "@deeptrust-ai/deep-ui",
  "author": {
    "name": "DeepTrust",
    "url": "https://www.deeptrust.ai/"
  },
  "homepage": "https://github.com/deeptrust-ai/deep-ui#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.3",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "vite build",
    "generate": "plop",
    "lint:library": "eslint \"lib/**/*.@(ts|tsx)\"",
    "lint:stories": "eslint \"**/*.stories.@(ts|tsx|mdx)\"",
    "lint": "npm run lint:library && npm run lint:stories",
    "tsc:library": "tsc -p tsconfig.lib.json --pretty",
    "tsc:stories": "tsc -p tsconfig.stories.json --pretty",
    "tsc": "npm run tsc:library && npm run tsc:stories",
    "format": "prettier --write .",
    "chromatic": "npx chromatic"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts",
        "dist/*"
      ]
    }
  },
  "peerDependencies": {
    "@phosphor-icons/react": "2.1.10",
    "@radix-ui/themes": "3.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deeptrust-ai/deep-ui.git"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.3",
    "@eslint/js": "9.38.0",
    "@mantine/hooks": "8.3.6",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/css-modules": "1.0.5",
    "@types/node": "24.9.2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "5.1.1",
    "@vitejs/plugin-react-swc": "4.2.0",
    "ajv": "8.17.1",
    "chromatic": "13.3.4",
    "classnames": "2.5.1",
    "date-fns": "4.1.0",
    "eslint": "9.38.0",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "eslint-plugin-storybook": "10.1.11",
    "glob": "13.0.0",
    "globals": "16.4.0",
    "playwright": "1.56.1",
    "plop": "4.0.1",
    "prettier": "3.6.2",
    "react": "^18.3.1",
    "react-day-picker": "9.13.0",
    "react-dom": "^18.3.1",
    "storybook": "10.1.11",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "7.2.4",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-lib-inject-css": "2.2.2"
  },
  "dependencies": {
    "@radix-ui/react-popover": "1.1.15"
  }
}
